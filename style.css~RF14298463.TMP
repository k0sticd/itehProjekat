            padding: 10px;
        }

        .datagrid table thead th {
            background: #595959;
            color: #ffffff;
            font-size: 15px;
            font-weight: bold;
            text-align: center;
            border-right: 1px solid #bfbfbf;
            border-bottom: 1px solid #bfbfbf;
        }

        .datagrid table tbody td {
            color: #404040;
            border-right: 1px solid #bfbfbf;
            border-bottom: 1px solid #bfbfbf;
            font-size: 12px;
        }

